{
  "kind": "BuildConfig",
  "apiVersion": "v1",
  "metadata": {
    "name": "mlbparks",
    "labels": {
      "application": "mlbparks",
      "category": "hands-on-lab"
    }
  },
  "spec": {
    "source": {
      "type": "Git",
      "git": {
        "uri": "https://github.com/jorgemoralespou/openshift3mlbparks.git",
        "ref": "master"
      },
      "contextDir":""
    },
    "strategy": {
      "type": "Source",
      "sourceStrategy": {
        "from": {
          "kind": "ImageStreamTag",
          "namespace": "openshift",
          "name": "jboss-eap6-openshift:6.4"
        }
      }
    },
    "output": {
      "to": {
        "kind": "ImageStreamTag",
        "name": "mlbparks:latest"
      }
    },
    "triggers": [
      {
        "type": "Github",
        "github": {
          "secret": "secret"
        }
      },
      {
        "type": "Generic",
        "generic": {
          "secret": "secret"
        }
      },
      {
        "type": "ImageChange",
        "imageChange": {}
      }
    ]
  }
}
